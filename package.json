{
  "name": "keep",
  "productName": "Keep",
  "version": "0.2.0",
  "description": "Google Keep menu bar app for macOS",
  "main": "main.js",
  "scripts": {
    "build": "npm run build:macos",
    "build:macos": "electron-packager . --electron-version 1.4.13 --icon=assets/icon.icns --ignore='^/(dist|assets)$' --out=dist --overwrite --platform=darwin --quiet",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": "https://github.com/tmcinerney/keep",
  "keywords": [
    "electron",
    "google",
    "keep"
  ],
  "author": "Travers McInerney",
  "license": "MIT",
  "dependencies": {
    "menubar": "^5.2.3"
  },
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.5.2"
  }
}
